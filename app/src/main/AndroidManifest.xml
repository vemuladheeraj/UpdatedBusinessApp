<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dh.retailbussiness" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.dh.retailbussiness.LoginActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.dh.retailbussiness.MainActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name="com.dh.retailbussiness.SignupActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name="com.dh.retailbussiness.AddDetailsActivity"
            android:theme="@style/AppTheme" />
    </application>

</manifest>
